  <% include ../views/partials/head.ejs %>
  <title>Home Page</title>
  <link rel="stylesheet" href= "../resources/css/myStyle.css">
  </head>
  <body>

    <% include ../views/partials/navbar.ejs %>
    <br><br><br>
    <div class="container" id="mainContainer">
      <h1 class="text text-center">Your Playlist</h1><br>
        <div class="row">
          <div class="col-md-8">
            <!--<div class="container" id="containerSongs">-->
              <iframe id="spotifyFrame" src="https://open.spotify.com/embed/playlist/<%- pLID %>" width="100%" height="380" frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
              <br><br>
              <div class="container text-center" id="spotifyUsernameBox">
                <h4> Spotify Username: <%- guestUsername %></h4>
                <p> Export Playlist to Spotify Account </p>
                <button class="btn btn-sm btn-primary" type="submit"> Export Playlist </button>

            </div>
          </div>
          <div class="col-md-4">
            <div class="container" id="locationBox">
              <!-- add JS to display info -->
              <h2>Location: <%- location %></h2>
              <p></p>
              <p> Current Tempurature: <%- currTemp %></p>
              <p> Minimum Tempurature: <%- minTemp %></p>
              <p> Maximum Tempurature: <%- maxTemp %></p>
           <script src="../resources/js/homescript.js"></script>
  
           <script>
            var id = <%-JSON.stringify(weatherId)%>;
            //changeBackground(id);
            keywordPicker(id);
           </script>

            </div>
          </div>
        </div>
    </div>
    <% include ../views/partials/footer.ejs %>

